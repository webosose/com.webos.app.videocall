{
  "name": "videocall",
  "version": "1.0.0",
  "description": "App to demostrate videocall features",
  "author": "",
  "main": "src/index.js",
  "scripts": {
    "serve": "enact serve",
    "pack": "enact pack",
    "pack-p": "enact pack -p",
    "watch": "enact pack --watch",
    "clean": "enact clean",
    "lint": "enact lint .",
    "license": "enact license",
    "test": "enact test",
    "test-watch": "enact test --watch"
  },
  "license": "UNLICENSED",
  "private": true,
  "repository": "",
  "engines": {
    "npm": ">=6.9.0"
  },
  "enact": {
    "theme": "sandstone",
    "deep": [
      "(window.PalmSystem && window.PalmSystem.launchParams && JSON.parse(window.PalmSystem.launchParams).target) || (window.localStorage.getItem('menuTransparency') && JSON.parse(window.localStorage.getItem('menuTransparency')).menuTransparency==='off') || (window.PalmSystem && window.PalmSystem.highContrast === 'on')"
    ]
  },
  "eslintConfig": {
    "extends": "enact"
  },
  "eslintIgnore": [
    "node_modules/*",
    "build/*",
    "dist/*"
  ],
  "dependencies": {
    "@enact/core": "^4.0.0",
    "@enact/i18n": "^4.0.0",
	"@enact/sandstone": "^2.0.0-alpha.3",
    "@enact/spotlight": "^4.0.0",
    "@enact/ui": "^4.0.0",
    "@enact/webos": "^4.0.0",
    "ilib": "npm:ilib-webos@^14.6.2-webos1",
    "prop-types": "^15.6.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
